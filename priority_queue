#include<bits/stdc++.h>
#include<conio.h>
using namespace std;
 
int main(){
system("cls");

    // priority_queue<int>pq; //Default Priority queue
    // priority_queue<int, vector<int>, greater<int>> pq;
    
    //using comparetor for accending order
    priority_queue<int, vector<int>, greater<int>> pq;
    pq.push(8);
    pq.push(4);
    pq.push(1);
    pq.push(3);
    pq.push(9);
    pq.push(2);

    //  cout<<pq.top(); //give max value

    while (!pq.empty())
    {
        cout << pq.top()<<endl;
        pq.pop();
    }

  cout << "\nsize: " << pq.size(); 
  getch();
  return 0;
}
